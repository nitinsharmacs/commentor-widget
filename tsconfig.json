{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "jsx": "react-jsx",
    "lib": ["DOM", "ES2018"],
    "module": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "typeRoots": ["./types"],
    "strictFunctionTypes": true,
    "useUnknownInCatchVariables": true
  }
}
